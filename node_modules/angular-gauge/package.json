{
  "name": "angular-gauge",
  "version": "4.0.0",
  "description": "A tiny angular 10.0+ wrapper for https://github.com/naikus/svg-gauge",
  "lint-staged": {
    "**/*.{ts,html}": [
      "npm run prettier"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattlewis92/angular-gauge.git"
  },
  "keywords": [
    "angular2",
    "angular",
    "angular4",
    "angular5",
    "gauge",
    "svg-gauge"
  ],
  "author": "Matt Lewis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattlewis92/angular-gauge/issues"
  },
  "homepage": "https://github.com/mattlewis92/angular-gauge#readme",
  "peerDependencies": {
    "@angular/core": ">=10.0.0"
  },
  "dependencies": {
    "svg-gauge": "^1.0.6",
    "tslib": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "main": "bundles/angular-gauge.umd.js",
  "module": "fesm2015/angular-gauge.js",
  "es2015": "fesm2015/angular-gauge.js",
  "esm2015": "esm2015/angular-gauge.js",
  "fesm2015": "fesm2015/angular-gauge.js",
  "typings": "angular-gauge.d.ts",
  "metadata": "angular-gauge.metadata.json",
